# e = 1 + sum(1/n!)
# e.g. e = 1 + 1/1 + 1/(1*2) + 1/(1*2*3) + ...
  push 1.0      # sum
  push 1.0      # n!
  push 1.0      # n

loop:
  # update sum
  swap 2
  push 1.0
  dup 2
  fdiv
  fplus
  swap 2

  # update n
  push 1.0
  fplus

  # update n!
  swap 1
  dup 1
  fmult
  swap 1

  dup 0
  push 10.0
  ge
  jz loop

  halt